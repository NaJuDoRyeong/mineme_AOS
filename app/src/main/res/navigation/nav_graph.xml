<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/home">
    <fragment
        android:id="@+id/story"
        android:name="com.najudoryeong.mineme.story.StoryFragment"
        android:label="StoryFragment" />
    <fragment
        android:id="@+id/home"
        android:name="com.najudoryeong.mineme.home.HomeFragment"
        android:label="HomeFragment" />
    <fragment
        android:id="@+id/setting"
        android:name="com.najudoryeong.mineme.setting.fragment.SettingFragment"
        android:label="SettingFragment" >
        <action
            android:id="@+id/action_setting_to_settingInquiryFragment"
            app:destination="@id/settingInquiryFragment" />
        <action
            android:id="@+id/action_setting_to_settingNoticeFragment"
            app:destination="@id/settingNoticeFragment" />
        <action
            android:id="@+id/action_setting_to_settingSecurityFragment"
            app:destination="@id/settingSecurityFragment" />
        <action
            android:id="@+id/action_setting_to_settingNotificationFragment"
            app:destination="@id/settingNotificationFragment" />
        <action
            android:id="@+id/action_setting_to_settingAccountFragment"
            app:destination="@id/settingAccountFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingAccountFragment"
        android:name="com.najudoryeong.mineme.setting.fragment.SettingAccountFragment"
        android:label="SettingAccountFragment" />
    <fragment
        android:id="@+id/settingInquiryFragment"
        android:name="com.najudoryeong.mineme.setting.fragment.SettingInquiryFragment"
        android:label="SettingInquiryFragment" />
    <fragment
        android:id="@+id/settingNoticeFragment"
        android:name="com.najudoryeong.mineme.setting.fragment.SettingNoticeFragment"
        android:label="SettingNoticeFragment" />
    <fragment
        android:id="@+id/settingNotificationFragment"
        android:name="com.najudoryeong.mineme.setting.fragment.SettingNotificationFragment"
        android:label="SettingNotificationFragment" />
    <fragment
        android:id="@+id/settingSecurityFragment"
        android:name="com.najudoryeong.mineme.setting.fragment.SettingSecurityFragment"
        android:label="SettingSecurityFragment" />
</navigation>